{
  "name": "jpeg-asm",
  "version": "1.0.0",
  "description": "Encoding and decoding images via libjpeg",
  "main": "lib/api.js",
  "scripts": {
    "test": "mocha test",
    "build:debug": "scripts/embuild.sh --lib=jpeg --purge --configure --make --debug && ./scripts/embuild.sh --lib=jpegasm --debug",
    "build:debug:fast": "scripts/embuild.sh --lib=jpegasm --debug",
    "build:release": "scripts/embuild.sh --lib=jpeg --purge --configure --make && ./scripts/embuild.sh --lib=jpegasm",
    "build-resize": "scripts/build_stb_image_resize.sh",
    "build": "npm run build:release",
    "browser": "gulp"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gchudnov/jpeg-asm.git"
  },
  "keywords": [
    "jpeg",
    "image",
    "libjpeg",
    "decode",
    "encode"
  ],
  "author": "Grigoriy Chudnov <g.chudnov@gmail.com> (https://github.com/gchudnov)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gchudnov/jpeg-asm/issues"
  },
  "homepage": "https://github.com/gchudnov/jpeg-asm",
  "devDependencies": {
    "async": "^1.5.0",
    "babel-cli": "^6.2.0",
    "babel-core": "^6.2.1",
    "babel-polyfill": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-stage-0": "^6.1.18",
    "browserify": "^12.0.1",
    "gulp": "^3.9.0",
    "gulp-if": "^2.0.0",
    "gulp-notify": "^2.2.0",
    "gulp-replace": "^0.5.4",
    "gulp-uglify": "^1.5.1",
    "gulp-util": "^3.0.7",
    "mocha": "^3.5.0",
    "once": "^1.3.3",
    "pretty-hrtime": "^1.0.1",
    "require-dir": "^0.3.0",
    "should": "^7.1.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  }
}
